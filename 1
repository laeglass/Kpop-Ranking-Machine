//轉換+++++++++++++++++++++++++++
function toUnit(n) {
	var str;
	var sw;
	switch(n) {
		case 1: sw=0;break;
		case 2: sw=0;break;
		case 3: sw=0;break;
		case 4: sw=0;break;
		case 5: sw=0;break;
		case 6: sw=1;break;
